<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_page">
    <sys_ui_page action="INSERT_OR_UPDATE">
        <category>general</category>
        <client_script/>
        <description/>
        <direct>false</direct>
        <endpoint>x_376096_mplc_overview.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
	<g:evaluate>
		const mplcUtil = new x_376096_mplc.MPLCUtil();
		const sources = mplcUtil.getGitHubSources();
		const searchString = RP.getParameterValue('search');
		if (searchString) {
			mplcUtil.searchGitSources(sources, searchString);
		}
	</g:evaluate>
	<div style="display: flex;">
	<j:forEach var="jvar_source" items="${sources}">
		<div style="width: 200px">
			<table>
				<tr>
					<td>${jvar_source.name}</td>
					<td>
						<j:if test="${jvar_source.valid == true}">
							<i class="icon icon-circle-solid"></i>
						</j:if>
						<j:if test="${jvar_source.valid == false}">
							<i class="icon icon-error-circle"></i>
						</j:if>
					</td>
				</tr>
				<tr><td colspan="2">${jvar_source.description}</td></tr>
			</table>
		</div>
	</j:forEach>
	</div>
</j:jelly>]]></html>
