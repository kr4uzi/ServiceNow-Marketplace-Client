        <order>100</order>
        <script><![CDATA[/* global current, action */
/* global gs, x_376096_mplc */
/* eslint no-undef: "error" */
(function (appGr, action) {
	try {
		const util = new x_376096_mplc.MPLCUtil();
		const res = util.syncApp(current);
		if (res.status != 'success') {
			gs.addErrorMessage(res.messages.join('\n'));
		} else {
			gs.addInfoMessage('Sync successful');
		}
	} catch (e) {
		gs.addErrorMessage('failed: ' + e +'\n' + e.stack);
	}

	action.setRedirectURL(appGr);

})(current, action);]]></script>
